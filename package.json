{
  "name": "brolab-beats-store",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "BroLab Entertainment - Modern Beats Store with WooCommerce Integration",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx -r dotenv/config server/index.ts",
    "dev:development": "cross-env NODE_ENV=development tsx -r dotenv/config server/index.ts",
    "client": "vite",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "pre-check": "npm run type-check && npm run lint",
    "pre-build": "npm run pre-check",
    "pre-commit": "node scripts/pre-commit-check.js",
    "verify": "npm run pre-commit",
    "setup": "npm install",
    "clean": "rimraf node_modules dist .cache && npm install",
    "test": "jest",
    "test:email": "tsx scripts/test-email.ts",
    "fix-subscriptions": "tsx scripts/fix-subscriptions.ts",
    "clean:all": "node scripts/clean-all.js",
    "clean:logs": "node scripts/clear-logs.js",
    "clean:db": "node scripts/clear-database-final.js",
    "generate-favicon": "node scripts/generate-favicon.js"
  },
